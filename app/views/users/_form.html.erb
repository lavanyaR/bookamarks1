<%= form_for(@user) do |f| %>
<% if @user.errors.any? %>
  <div id="error_explanation">
    <h2><%= pluralize(@user.errors.count, "error") %> title cannot be blank we need to enter bookmarks:</h2>

    <ul>
    <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
 <% end %>
  <%= f.label :name %>
  <%= f.text_field :name %> </br>
  <%= f.label :email %>
  <%= f.text_field :email %> </br>
  <%= f.label :password %>
  <%= f.password_field :password %> </br>
  confirmpassword
  <%= password_field_tag :confirmpassword %> 
  <%= f.submit "submit"%> </br>
<% end %>

